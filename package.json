{
  "name": "concept-grapher",
  "version": "0.0.1",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc && node-sass --include-path scss scss/main.scss dist/css/main.css",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js"
  },
  "repository": "https://github.com/Odin94/concept-grapher.git",
  "keywords": [
    "Electron",
    "concept",
    "map",
    "concept map",
    "mind map",
    "typescript"
  ],
  "author": "Odin94",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/pixi.js": "^4.8.0",
    "electron": "^2.0.8",
    "node-sass": "^4.9.3",
    "pixi-viewport": "^3.6.0",
    "pixi.js": "^4.8.1",
    "tslint": "^5.10.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {}
}
